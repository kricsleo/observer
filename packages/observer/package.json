{
  "name": "@kricsleo/observer",
  "type": "module",
  "version": "0.1.11",
  "packageManager": "pnpm@7.32.2",
  "description": "easy to use intersection observer",
  "keywords": [
    "IntersectionObserver",
    "vue",
    "react",
    "js",
    "lazy-load",
    "exposure"
  ],
  "author": "kricsleo <kricsleo@163.com>",
  "license": "MIT",
  "homepage": "https://github.com/kricsleo/pkg-name#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kricsleo/pkg-name.git"
  },
  "bugs": "https://github.com/kricsleo/pkg-name/issues",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish",
    "start": "tsx src/index.ts",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@antfu/ni": "^0.21.3",
    "@types/node": "^18.16.0",
    "bumpp": "^9.1.0",
    "pnpm": "^7.32.2",
    "tsx": "^3.12.6",
    "typescript": "^5.0.4",
    "unbuild": "^1.2.1"
  }
}
